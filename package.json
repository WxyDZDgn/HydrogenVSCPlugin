{
  "name": "hydrogen",
  "displayName": "Hydrogen",
  "description": "Hydrogen is a Turing complete programming language.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
